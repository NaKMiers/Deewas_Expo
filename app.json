{
  "expo": {
    "name": "deewas-expo",
    "slug": "deewas-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "deewas",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "extra": {
      "webServerUrl": "http://localhost:3000",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f958fd50-8044-4f35-bca1-f336977979ef"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anphagroup.deewas",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anphagroup.deewas"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
