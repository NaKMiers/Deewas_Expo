{
  "expo": {
    "name": "deewas-expo",
    "slug": "deewas-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "deewas",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#161616"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "webServerUrl": "http://localhost:3000",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e65bb9d5-fa57-4126-94c5-bc79f840eae8"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anphagroup.deewas",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#161616"
      },
      "package": "com.anphagroup.deewas"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
